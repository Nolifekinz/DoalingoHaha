<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/showKQ"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ShowKetQuaTest">

    <TextView
        android:id="@+id/hienthiketqua"
        android:layout_width="413dp"
        android:layout_height="122dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.458"
        android:fontFamily="@font/duolingo_fontfamily"
        android:textSize="20dp"
        android:gravity="center"/>

    <Button
        android:id="@+id/didenbaihoc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="132dp"
        android:text="Đi đến bài học"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/hienthiketqua" />

</androidx.constraintlayout.widget.ConstraintLayout>